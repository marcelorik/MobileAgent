

public class ServiceHello implements ServiceI
{ 
    //Tipo serviço pré definido
    private int type = 1;

    public ServiceHello(){
      super();
     }
     
    public ServiceHello(int type){
      super();
      this.type = type;
    }
 
    public void doService()
    {         
      System.out.print("\nHello World!\n"); 
    }
    
    public int typeService(){
      return type;
    }

}
